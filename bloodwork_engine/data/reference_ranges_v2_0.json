          "condition": "zinc_serum / copper_serum > threshold",
          "threshold_ratio": 1.5,
          "unit": "ratio",
          "action": "CAUTION_ZINC_EXCESS",
          "effect": "reduce_zinc_dosing",
          "rationale": "Excess zinc depletes copper; maintain 1:1 ratio approximately"
        },
        {
          "gate_id": "GATE_018",
          "name": "insulin_resistance",
          "trigger_marker": ["fasting_insulin", "fasting_glucose"],
          "condition": "fasting_insulin > 10 OR homa_ir > 2.5",
          "insulin_threshold": 10,
          "homa_ir_threshold": 2.5,
          "unit": "µIU/mL | ratio",
          "action": "FLAG_INSULIN_SUPPORT",
          "recommended_ingredients": ["berberine", "chromium", "alpha_lipoic_acid", "inositol"],
          "calculation_note": "HOMA-IR = (fasting_glucose * fasting_insulin) / 405",
          "rationale": "Insulin resistance benefits from targeted metabolic support"
        },
        {
          "gate_id": "GATE_019",
          "name": "iron_deficiency_anemia",
          "trigger_marker": ["hemoglobin", "ferritin"],
          "condition": "hemoglobin < anemia_threshold AND ferritin < ferritin_threshold",
          "hemoglobin_threshold_male": 13.0,
          "hemoglobin_threshold_female": 11.0,
          "ferritin_threshold": 30,
          "unit": "g/dL | ng/mL",
          "action": "FLAG_IRON_DEFICIENCY_ANEMIA",
          "effect": "override_iron_block",
          "recommended_ingredients": ["iron_bisglycinate", "vitamin_c"],
          "rationale": "True IDA requires iron despite elevated ferritin inflammation"
        },
        {
          "gate_id": "GATE_020",
          "name": "triglyceride_caution",
          "trigger_marker": "triglycerides",
          "condition": "triglycerides > threshold",
          "threshold": 500,
          "unit": "mg/dL",
          "action": "CAUTION_FISH_OIL_DOSE",
          "effect": "limit_fish_oil_4g_max",
          "rationale": "Very high TG may require prescription-strength omega-3; start conservative"
        }
      ]
